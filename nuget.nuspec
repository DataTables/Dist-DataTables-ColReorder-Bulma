<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>2.0.2</version>
        <id>datatables.net-colreorder-bm</id>
        <description>This is ColReorder for DataTables with styling for [Bulma](https://bulma.io/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-ColReorder-Bulma.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>ColReorder  Reorder  Bulma  Datatables jQuery table</tags>
        <dependencies>
            <dependency id="datatables.net-colreorder" version="1.5.6"/>
            <dependency id="datatables.net-bm" version="1.12.1"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content"/>
        </contentFiles>
    </metadata>
    <files>
        <file src="js/colReorder.bulma.js" target="content\Scripts"/>
        <file src="js/colReorder.bulma.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/colReorder.bulma.min.js" target="content\Scripts"/>
        <file src="js/colReorder.bulma.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/colReorder.bulma.css" target="content\Content"/>
        <file src="css/colReorder.bulma.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/colReorder.bulma.min.css" target="content\Content"/>
        <file src="css/colReorder.bulma.min.css" target="contentFiles\any\any\wwwroot\css"/>
    </files>
</package>

